{
  "name": "flensburgia-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "coverage:check": "node -e \"require('fs').mkdirSync('coverage/tmp', { recursive: true });\" && npx c8 check-coverage --branches 50 --lines 70 --functions 60 --statements 70"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "lucide-vue-next": "^0.509.0",
    "markdown-it": "^14.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.5",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.30",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.2.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-standard": "^9.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.2",
    "vue-eslint-parser": "^10.0.0"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint:fix"
  }
}
